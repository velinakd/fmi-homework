import java.io.IOException;
import java.util.Scanner;

public class TicTacToe {

	private int max(int A, int B, Node state) {
		if (state.isFinal()) {
			return state.evaluate(); // who wins and with how many empty slots
			// e.g. first wins, 3 empty = 4; second wins, 4 empty = -1+ -4=-5
		}
		for (int i = 0; i < 9; i++) {
			if (state.isValid(i)) {
				state.doMove(i, 1);
				int x = min(A, B, state);
				if (A < x) {
					A = x;
				}
				if (A >= B) {
					return A;
				}
				state.revertMove(i);
			}
		}
		return A; //TODO
	}

	private int min(int A, int B, Node state) {
		if(state.isFinal()) {
			return state.evaluate();
		}
		for (int i = 0; i < 9; i++) {
			if (state.isValid(i)) {
				state.doMove(i, 2);
				int x = max(A,B,state);
				if (B > x) {
					B = x;
				}
				if (A >= B) {
					return B;
				}
				state.revertMove(i);
			}
		}
		return B; //TODO
	}
	
	public void minimax() {
		Node startNode = new Node();
		max(10, -10, startNode);
	}

	public static void main(String[] args) {
		Node state = new Node();
		state.print();
		Scanner in = new Scanner(System.in);
		int move = in.nextInt();
		state.doMove(move, 2);
		state.print();
	}

}
