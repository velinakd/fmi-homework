
public class Node {
	private int[][] board;
	
	public int evaluate() {
		return 1;
	}
	
	public boolean isFinal() {
		for(int i=0; i<3; i++) {
			if(board[i][0] == board[i][1] && board[i][1] == board[i][2])
				return true;
			if(board[0][i] == board[1][i] && board[1][i] == board[2][i])
				return true;
		}
		if((board[0][0] == board[1][1] && board[1][1] == board[2][2]) ||
			(board[0][2] == board[1][1] && board[1][1] == board[2][0]))
			return true;
		return false;
	}
}
